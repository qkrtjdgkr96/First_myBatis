<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="NoticeMapper">
  	<select id="noticeView" resultType="com.choa.notice.NoticeDTO">
  		<![CDATA[select * from notice_board where num=#{num}]]>
  	</select>
  	
  	<insert id="noticeWrite" parameterType="">
  		insert into notice_board values(notice_seq.nextVal, #{writer}, #{title}, #{contents}, 0, sysdate);
  	</insert>
  	
  	<select id="" resultType="">
  		
  	</select>
  	
  	
  </mapper>